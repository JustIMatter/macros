<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--                                                                                    -->
<!-- GUI layout configuration for mod_macros. Do not edit this file by hand, unless you -->
<!-- want to, in which case go ahead. But don't blame me if it formats your hard drive. -->
<!--                                                                                    -->
<gui xmlns:gb="http://eq2online.net/macros/guibinding" xmlns:gc="http://eq2online.net/macros/guiconfiguration">
    <!-- GUI slot bindings -->
    <gb:bindings>
        <gb:binding slot="playback">default</gb:binding>
        <gb:binding slot="scoreboard">ingame</gb:binding>
        <gb:binding slot="indebug">ingame</gb:binding>
        <gb:binding slot="inchat">inchat</gb:binding>
        <gb:binding slot="ingame">ingame</gb:binding>
    </gb:bindings>
    <!-- Config for layout 'default' -->
    <gc:guilayout columns="6" display="default" dots="false" grid="false" name="default" padding="2" rows="10">
        <gc:controls/>
    </gc:guilayout>
    <!-- Config for layout 'inchat' -->
    <gc:guilayout columns="6" display="inchat" dots="false" grid="false" name="inchat" padding="2" rows="10">
        <gc:controls/>
    </gc:guilayout>
    <!-- Config for layout 'ingame' -->
    <gc:guilayout columns="4" display="ingame" dots="false" grid="false" name="ingame" padding="1" rows="8">
        <gc:metrics>
            <gc:column id="0" width="60"/>
        </gc:metrics>
        <gc:controls>
            <gc:textarea id="256" name="debug" rowspan="4" x="3" y="4">
                <colour>#00FF00</colour>
                <lifespan>200</lifespan>
                <visible>1</visible>
            </gc:textarea>
            <gc:label id="257" name="armordura" x="0" y="0">
                <align>top left</align>
                <background>#AA000000</background>
                <binding/>
                <colour>#00FF00</colour>
                <shadow>1</shadow>
                <text>&amp;4Helmet: 0\n&amp;cChest: 144\n&amp;4Legs: 0\n&amp;cBoots: 99</text>
                <visible>1</visible>
            </gc:label>
        </gc:controls>
    </gc:guilayout>
</gui>
